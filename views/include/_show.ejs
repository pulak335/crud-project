<% for (var i=0;i< users.length;i++){%>
<tr>
    <td><% =i+1 %></td>
    <td><% =user[i].name %></td>
    <td><% =user[i].email %></td>
    <td><% =user[i].gender %></td>
    <td><% =user[i].status %></td>
    <td>
      <a href="/update-user?id=<% =users[i]._id %>" class="btn border-shadow update">
        <span class="text-gradint"><i class="fas fa-user-edit"></i></span>
      </a>
      <a href="#" class="btn border-shadow delete" data-id=<% =users[i]._id %>>
          <span class="text-gradint"><i class="fas fa-trash"></i></span>
      </a>
    </td>
  </tr>